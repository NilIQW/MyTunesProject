            <Button fx:id="saveBtn" layoutX="434.0" layoutY="340.0" maxHeight="-Infinity" maxWidth="-Infinity" mnemonicParsing="false" onAction="#Save" prefHeight="26.0" prefWidth="68.0" style="-fx-background-color: #BAD6FF;" text="Save" />
            </children>
        </AnchorPane>
    </children>
</AnchorPane>
