                    <Button mnemonicParsing="false" style="-fx-background-color: #E5DFF6;" text="↑">
                        <HBox.margin>
                            <Insets right="5.0" top="3.0"/>
                        </HBox.margin>
                    </Button>
                    <Button mnemonicParsing="false" style="-fx-background-color: #E5DFF6;" text="↓">
                        <HBox.margin>
                            <Insets left="5.0" right="5.0" top="3.0"/>
                        </HBox.margin>
                    </Button>
                    <Button mnemonicParsing="false" style="-fx-background-color: #E5DFF6;" text="Delete">
                        <HBox.margin>
                            <Insets left="45.0" top="3.0"/>
                        </HBox.margin>
                    </Button>
                </HBox>
            </children>
        </VBox>
        <VBox layoutX="366.0" layoutY="199.0" prefHeight="200.0" prefWidth="45.0">
            <Button mnemonicParsing="false" style="-fx-background-color: #E5DFF6;" text="←">
