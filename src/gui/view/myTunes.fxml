                </VBox.margin>
            </Button>
        </VBox>
        <VBox layoutX="411.0" layoutY="175.0" prefHeight="489.0" prefWidth="399.0">
            <children>
                <Label text="Songs">
                    <VBox.margin>
                        <Insets bottom="3.0" />
                    </VBox.margin>
                </Label>
                <ListView fx:id="songListView" prefHeight="383.0" prefWidth="387.0" style="-fx-background-color: #E5DFF6;" />
                <HBox prefHeight="100.0" prefWidth="200.0">
                    <children>
                        <Button mnemonicParsing="false" onAction="#openNewSong" style="-fx-background-color: #E5DFF6;" text="New...">
                            <HBox.margin>
                                <Insets right="5.0" top="3.0" />
                            </HBox.margin>
                        </Button>
                        <Button mnemonicParsing="false" style="-fx-background-color: #E5DFF6;" text="Edit...">
                            <HBox.margin>
                                <Insets left="5.0" right="5.0" top="3.0" />
                            </HBox.margin>
                        </Button>
                        <Button mnemonicParsing="false" style="-fx-background-color: #E5DFF6;" text="Delete">
                            <HBox.margin>
                                <Insets left="5.0" right="5.0" top="3.0" />
                            </HBox.margin>
                        </Button>
                        <Button mnemonicParsing="false" style="-fx-background-color: #E5DFF6;" text="Close">
                            <HBox.margin>
                                <Insets left="170.0" top="3.0" />
                            </HBox.margin>
                        </Button>
                    </children>
                    <VBox.margin>
                        <Insets />
                    </VBox.margin>
                </HBox>
            </children>
        </VBox>
    </children>
</AnchorPane>
