<?import javafx.scene.text.Font?>

<AnchorPane style="-fx-background-color: #C9BFE5;" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.controller.MyTunesController">
    <children>
        <HBox layoutY="1.0" prefHeight="178.0" prefWidth="822.0">
            <AnchorPane prefHeight="200.0" prefWidth="200.0">
                <Button layoutX="32.0" layoutY="62.0" mnemonicParsing="false" prefHeight="33.0" prefWidth="35.0"
                        style="-fx-background-radius: 16; -fx-background-color: #E5DFF6;" text="◃◃"/>
                <Button layoutX="73.0" layoutY="53.0" mnemonicParsing="false" prefHeight="50.0" prefWidth="50.0"
                        style="-fx-background-radius: 36; -fx-background-color: #E5DFF6;" text="▶"/>
                <Button layoutX="129.0" layoutY="61.0" mnemonicParsing="false" prefHeight="33.0" prefWidth="35.0"
                        style="-fx-background-color: #E5DFF6; -fx-background-radius: 16;" text="▹▹"/>
                <Slider layoutX="41.0" layoutY="128.0" prefHeight="16.0" prefWidth="114.0"
                        style="-fx-background-color: #C9BFE5;"/>
                <Label layoutX="17.0" layoutY="121.0" text="🔊">
                    <font>
                        <Font size="19.0"/>
                    </font>
                </Label>
            </AnchorPane>
            <HBox prefHeight="178.0" prefWidth="329.0"/>
            <HBox prefHeight="178.0" prefWidth="293.0">
                <children>
                    <Label text="Filter:">
                        <HBox.margin>
                            <Insets top="30.0"/>
                        </HBox.margin>
                    </Label>
                    <TextField prefHeight="26.0" prefWidth="174.0">
                        <HBox.margin>
                            <Insets left="8.0" top="30.0"/>
                        </HBox.margin>
                    </TextField>
                    <Button mnemonicParsing="false" text="🔍">
                        <HBox.margin>
                            <Insets left="10.0" top="30.0"/>
                        </HBox.margin>
                    </Button>
                </children>
            </HBox>
        </HBox>
        <VBox layoutX="14.0" layoutY="179.0" prefHeight="481.0" prefWidth="171.0">
            <children>
                <Label text="Playlists">
                    <VBox.margin>
                        <Insets bottom="3.0" />
                    </VBox.margin>
                </Label>
                <ListView prefHeight="382.0" prefWidth="181.0" style="-fx-background-color: #E5DFF6;" />
                <HBox prefHeight="100.0" prefWidth="200.0">
                    <children>
                        <Button fx:id="newPlaylist" mnemonicParsing="false" onAction="#newPlaylist" style="-fx-background-color: #E5DFF6;" text="New...">
                            <HBox.margin>
                                <Insets right="5.0" top="3.0" />
                            </HBox.margin>
                        </Button>
                        <Button fx:id="editPlaylist" mnemonicParsing="false" onAction="#editPlaylist" style="-fx-background-color: #E5DFF6;" text="Edit...">
                            <HBox.margin>
                                <Insets left="5.0" right="5.0" top="3.0" />
                            </HBox.margin>
                        </Button>
                        <Button fx:id="deletePlaylist" mnemonicParsing="false" style="-fx-background-color: #E5DFF6;" text="Delete">
                            <HBox.margin>
                                <Insets left="5.0" top="3.0" />
                            </HBox.margin>
                        </Button>
                    </children>
                </HBox>
            </children>
        </VBox>
        <VBox layoutX="195.0" layoutY="179.0" prefHeight="481.0" prefWidth="171.0">
            <children>
                <Label text="Songs on Playlist">
                    <VBox.margin>
                        <Insets bottom="3.0" />
                    </VBox.margin>
                </Label>
                <ListView prefHeight="380.0" prefWidth="171.0" style="-fx-background-color: #E5DFF6;" />
